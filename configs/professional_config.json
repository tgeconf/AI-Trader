{
  "agent_type": "BaseAgent",
  "date_range": {
    "init_date": "2025-10-01",
    "end_date": "2025-10-21"
  },
  "models": [
    {
      "name": "claude-3.7-sonnet",
      "basemodel": "anthropic/claude-3.7-sonnet",
      "signature": "claude-3.7-sonnet",
      "enabled": false
    },
    {
      "name": "deepseek-chat-v3.1",
      "basemodel": "deepseek/deepseek-chat-v3.1",
      "signature": "deepseek-chat-v3.1",
      "enabled": false
    },
    {
      "name": "qwen3-max",
      "basemodel": "qwen/qwen3-max",
      "signature": "qwen3-max",
      "enabled": false
    },
    {
      "name": "gemini-2.5-flash",
      "basemodel": "google/gemini-2.5-flash",
      "signature": "gemini-2.5-flash",
      "enabled": false
    },
    {
      "name": "gpt-5",
      "basemodel": "openai/gpt-5",
      "signature": "gpt-5",
      "enabled": true
    }
  ],
  "agent_config": {
    "max_steps": 30,
    "max_retries": 3,
    "base_delay": 1.0,
    "initial_cash": 10000.0
  },
  "log_config": {
    "log_path": "./data/agent_data"
  },
  "professional_modules": {
    "risk_management": {
      "enabled": true,
      "var_confidence_level": 0.95,
      "max_risk_per_trade": 0.02,
      "max_portfolio_risk": 0.10,
      "stop_loss_percentage": 0.05,
      "position_sizing_method": "kelly"
    },
    "trading_costs": {
      "enabled": true,
      "commission_rate": 0.001,
      "slippage_model": "volume_adjusted",
      "market_impact_enabled": true,
      "optimize_execution": true
    },
    "portfolio_optimization": {
      "enabled": true,
      "optimization_method": "mean_variance",
      "objective": "maximize_sharpe",
      "rebalancing_frequency": "weekly",
      "transaction_cost_aware": true
    },
    "multi_timeframe": {
      "enabled": true,
      "timeframes": ["1d", "4h", "1h", "15min"],
      "consensus_threshold": 0.7,
      "conflict_resolution": "weighted_average",
      "backtesting_enabled": true
    }
  },
  "risk_parameters": {
    "max_drawdown_limit": 0.15,
    "volatility_target": 0.12,
    "var_monitoring": true,
    "stress_testing": true,
    "liquidity_requirements": {
      "min_cash_ratio": 0.1,
      "max_position_concentration": 0.05
    }
  },
  "performance_metrics": {
    "track_sharpe_ratio": true,
    "track_max_drawdown": true,
    "track_win_rate": true,
    "track_profit_factor": true,
    "benchmark_comparison": "SPY",
    "attribution_analysis": true
  }
}
